<aside id="friendsColumn">
  <div class="scrollUp"><div class="arrow-up scroll-up"></div></div>
    <ul>
      <% for user in users %>
        <% if user.name != current_user1.name %>
            <% if Identity.where(:id => current_user1.id).first.image.nil? %>
              <li><img src="images/icons/image_not_found.png" data-user="<%= user.id %>" /></li>
            <% else %>
            <li>
              <%= link_to user %>
            </li>
            <% end %>
            <li>
              <%= link_to user.name, user %>
            </li>
        <% end %>
      <% end %>
    </ul>
  <div class="scrollDown"><div class="arrow-down scroll-down"></div></div>
</aside>